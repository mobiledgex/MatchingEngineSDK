  pod 'GoogleSignIn'

  pod 'SwiftLog'
  pod 'NSLogger/Swift'
  
  pod 'SPPermission'
  pod 'SwiftSocket'
  
  target 'MatchingEngine_Tests' do
    inherit! :search_paths
    
  end
  
end
