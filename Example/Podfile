use_frameworks!

platform :ios, '11.4'

# Default Specs.git:
source 'https://github.com/CocoaPods/Specs.git'
# MobiledgeX Private Trunk for Specs.git equivalent, , if not using "../":
source 'https://github.com/mobiledgex/PrivatePodTrunk.git'

